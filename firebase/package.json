{
  "name": "backend_genesis",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "setup": "yarn firebase use --add",
    "build": "babel 'functions' --out-dir 'babeled_functions' --source-maps --copy-files --presets=@babel/preset-env && cd babeled_functions && yarn",
    "watch": "yarn build --watch",
    "serve": "yarn watch && yarn firebase serve --only functions",
    "predeploy": "yarn build",
    "deploy": "yarn firebase deploy"
  },
  "engines": {
    "node": "8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "author": "Gon√ßalo Pereira <46056672+goncalomp@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/preset-env": "^7.3.1",
    "firebase-tools": "^6.10.0"
  },
  "main": "index.js",
  "repository": "https://github.com/jeKnowledge/backend_genesis.git"
}
